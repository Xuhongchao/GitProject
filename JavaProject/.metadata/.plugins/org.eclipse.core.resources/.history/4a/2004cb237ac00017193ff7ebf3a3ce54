package Entrance;

public class AppendNum {
	public static void main(String[] args) {
		String[] strs = { "6.0.1", "6.1.5  ", "       ", "       ", "       ",
				"       ", "       ", "4.0.0  ", "3.0.0  ", "5.0.0  ",
				"5.1.3  " };

		for (int i = 0; i < strs.length; i++) {
			strs[i] = strs[i].trim();
			if (strs[i] != null && !"".equals(strs[i].trim())) {
				String string = "ËÑºüÆû³µ" + strs[i];
				System.out.println(string);
			}

		}
	}
}
