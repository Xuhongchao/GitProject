public class Test2 {
	public static void main(String[] args) {
		ListNode head = new ListNode(1);
		ListNode head = new ListNode(1);
		ListNode head = new ListNode(1);
		ListNode head = new ListNode(1);
		ListNode head = new ListNode(1);
	}

	public static ListNode reverseList(ListNode head) {
		ListNode prev = null;
		while (head != null) {
			ListNode tmp = head.next;
			head.next = prev;
			prev = head;
			head = tmp;
		}
		return prev;
	}
}

class ListNode {
	private Object data;
	ListNode next;

	public ListNode(Object data) {
		super();
		this.data = data;
	}
}
