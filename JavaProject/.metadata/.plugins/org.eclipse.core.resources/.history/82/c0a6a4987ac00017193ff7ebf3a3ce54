                "",
                "",
                "",
                "",
                "1",
                "",
                "",
				"",
                "2.0.1",
				"",
                "",
                "",
                "",
				"",
                "",
                "",
				"",
				"",
                 "4.1.5"
			
				};

		for (int i = 0; i < strs.length; i++) {
			strs[i] = strs[i].trim();
			if (strs[i] != null && !"".equals(strs[i].trim())) {
				String string = "°®¿¨Æû³µ" + strs[i];
				System.out.println(string);
			}

		}
	}
}
