package Entrance;

public class AppendNum {
	public static void main(String[] args) {
		String[] nums = { "3.0.0",

		"5.4.4.1", "4.0.0",

		"5.0.0.0",

		"2.2.1",

		"3.4.0.2"

		};
		for (int i = 0; i < nums.length; i++) {
			String string = "太平洋汽车网" + nums[i];
			System.out.println(string);
		}
		
		
		String str = "
		携程旅行 2.3
		携程无线 3.0

		携程无线 4.0
		
		携程旅行 5.0
		
		携程旅行 6.0
		
		携程旅行 6.3.0
		
		携程旅行 6.4.5
		
		携程旅行 6.6.0
		
		携程旅行 6.7.0
		
		携程旅行 6.8.0
		
		携程旅行 6.9.0
		
		携程旅行 6.10.2
		
		携程旅行 6.13.0
		
		携程旅行 7.0.0
		";

	}
}
