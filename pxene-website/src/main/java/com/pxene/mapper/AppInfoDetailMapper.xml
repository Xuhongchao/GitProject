<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pxene.dao.AppInfoDetailMapper">

    <delete id="delDetail" parameterType="java.util.Map">
        delete from app_info_detail where app_info_num='${appNum}';
    </delete>

    <select id="queryByNum" parameterType="java.util.Map" resultType="java.util.Map">
        select * from app_info_detail where app_info_num='${app_num}'
    </select>

    <insert id="addDetail" parameterType="java.util.Map">
        insert into app_info_detail(app_info_num,app_is_major,app_crawl_id,app_os) values('${app_info_num}','${app_is_major}','${app_crawl_id}','${app_os}')
    </insert>
    
    <select id="queryByAppCrawlId" parameterType="java.util.Map" resultType="java.util.Map">
        select * from app_info_detail where app_crawl_id='${app_crawl_id}'
    </select>
    <update id="updateIsMajor" parameterType="java.util.Map">
        update app_info_detail set app_is_major='${app_is_major}' where app_info_num='${app_info_num}';
    </update>
    
</mapper>